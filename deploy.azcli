# Create / Update the deployment
az deployment sub create -n 'FinDep' -l 'westus' -f main.bicep

# Attach ACR
az aks update -n finlockeraks -g finlocker-RG --attach-acr finlockeracr

# Get AKS Credentials
az aks get-credentials -n finlockeraks -g finlocker-RG

# Cleanup. Delete the RG and Deployment
az group delete -g 'finlocker-RG' -y
az deployment sub delete -n 'FinDep'